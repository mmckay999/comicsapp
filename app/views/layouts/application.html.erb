<!DOCTYPE html>
<html>
  <head>
    <title>Comicsapp</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class= "navbar navbar-default navbar-static-top" role="navigation">
      <div class= 'container'>
        <div class= "navbar-header">
         <nav class= "navbar-brand">Mike's Comics</nav>
        </div>
       <links><%= link_to 'View All', comics_path, 
          class: "navbar-text navbar-right navbar-link"%>
          <%= link_to 'Add Comic', new_comic_path(@comics), 
          class: "navbar-text navbar-right navbar-link"%>
       </links>
      <div class= "collapse navbar-collapse" id="main-nav-collapse">
      </div>
    </div>
  </nav>

    <%= yield %>
  </body>
</html>

